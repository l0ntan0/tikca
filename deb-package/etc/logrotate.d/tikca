/var/log/tikca.log {
	weekly
	rotate 60
	compress
	delaycompress
	missingok
	notifempty
	create 644 root root
	size 20M
}

/var/log/tikca_comm.log {
        weekly
        rotate 60
        compress
        delaycompress
        missingok
        notifempty
        create 644 root root
        size 20M
}

